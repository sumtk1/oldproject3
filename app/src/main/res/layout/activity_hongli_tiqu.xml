<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cl_background">


    <include
        android:id="@+id/include3"
        layout="@layout/include_head">

    </include>

    <android.support.design.widget.TabLayout
        android:id="@+id/tl_hongli_tiqu_head"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/include3"
        app:tabIndicatorColor="@color/cl_activity_main"
        app:tabSelectedTextColor="@color/cl_activity_main"
        app:tabTextColor="@color/cl_666" />

    <com.gloiot.hygoSupply.ui.widget.NoScrollViewPager
        android:id="@+id/vp_hongli_tiqu_page"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tl_hongli_tiqu_head" />


</RelativeLayout>
