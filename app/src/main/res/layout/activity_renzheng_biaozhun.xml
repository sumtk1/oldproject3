<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_renzheng_biaozhun"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cl_bg_gray">

    <include
        android:id="@+id/include11"
        layout="@layout/include_head" />

    <View
        android:id="@+id/view"
        style="@style/title_view"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_below="@id/include11" />

    <ListView
        android:id="@+id/lv_renzheng_biaozhun"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/include11"
        android:divider="#00000000"
        android:dividerHeight="30px"
        android:visibility="gone" />

    <RelativeLayout
        android:id="@+id/rl_fg_wushuju"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_gravity="center"
        android:visibility="gone">

        <ImageView
            android:id="@+id/iv_hongli_mingxi_no_zhangdan_img"
            android:layout_width="92px"
            android:layout_height="106px"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center"
            android:src="@mipmap/ic_no_zhangdan"
            android:visibility="visible" />

        <TextView
            android:id="@+id/tv_hongli_mingxi_no_zhangdan_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/iv_hongli_mingxi_no_zhangdan_img"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center"
            android:layout_marginTop="40px"
            android:text="暂无数据"
            android:textColor="#666"
            android:textSize="32px"
            android:visibility="visible" />

    </RelativeLayout>


</RelativeLayout>
