<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/cl_background"
        android:visibility="visible">

        <LinearLayout
            android:id="@+id/linearLayout_hongbao_mingxi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="10px"
            android:background="@color/cl_background"
            android:gravity="right"
            android:orientation="horizontal"
            android:paddingLeft="30px">

            <Button
                android:id="@+id/btn_hongli_tiqu_hongbao_mingxi"
                android:layout_width="150px"
                android:layout_height="40px"
                android:layout_marginRight="30px"
                android:background="@drawable/bg_btn_confirm"
                android:text="红包明细"
                android:textColor="@color/cl_white" />

        </LinearLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout_hongli_tiqu_honglihongbao"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linearLayout_hongbao_mingxi"
            android:layout_marginTop="10px"
            android:background="@color/cl_background"
            android:gravity="center_horizontal">

            <TextView
                android:id="@+id/tv_hongli_tiqu_hongli_hongbao"
                style="@style/style_all_tv"
                android:layout_width="664px"
                android:layout_height="80px"
                android:background="#E33333"
                android:gravity="center"
                android:text="红利红包"
                android:textColor="@color/cl_white" />

            <ImageView
                android:layout_width="664px"
                android:layout_height="217px"
                android:layout_below="@+id/tv_hongli_tiqu_hongli_hongbao"
                android:src="@mipmap/ic_hongli_tiqu_hongli_hongbao" />

        </RelativeLayout>

        <LinearLayout
            android:id="@+id/linearLayout_ketiqu_hongli"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/relativeLayout_hongli_tiqu_honglihongbao"
            android:layout_marginTop="20px"
            android:background="#FFFFFF"
            android:orientation="vertical"
            android:paddingLeft="30px"
            android:paddingRight="30px">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="80px"
                android:focusable="true"
                android:focusableInTouchMode="true">

                <TextView
                    android:id="@+id/tv_hongli_tiqu_hongli_ketiqu_hongli"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:gravity="center_vertical"
                    android:text="可提取红利"
                    android:textColor="#333333"
                    android:textSize="32px" />

                <EditText
                    android:id="@+id/et_my_tiqu_zhuanchushouyi2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="40px"
                    android:background="@null"
                    android:hint="0.00"
                    android:textColorHint="#666"
                    android:textSize="32px" />
            </RelativeLayout>

            <include layout="@layout/include_view" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80px">

                <TextView
                    android:id="@+id/tv_guding_tiqu"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:gravity="center_vertical"
                    android:text="固定提取"
                    android:textColor="@color/cl_black"
                    android:textSize="32px" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="40px"
                    android:layout_toEndOf="@+id/tv_guding_tiqu"
                    android:layout_toRightOf="@+id/tv_guding_tiqu"
                    android:text="100"
                    android:textColor="#ff2e0d"
                    android:textSize="32px" />


            </RelativeLayout>

            <include layout="@layout/include_view" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80px"
                android:focusable="true"
                android:focusableInTouchMode="true">

                <TextView
                    android:id="@+id/tv_shuru_mima"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:gravity="center_vertical"
                    android:text="密码"
                    android:textColor="@color/cl_black"
                    android:textSize="32px" />

                <EditText
                    android:id="@+id/et_hongli_tiqu_qingshuru_mima"
                    style="@style/style_all_et"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="40px"
                    android:layout_toEndOf="@+id/tv_shuru_mima"
                    android:layout_toRightOf="@+id/tv_shuru_mima"
                    android:background="@null"
                    android:hint="请输入密码" />


            </RelativeLayout>


        </LinearLayout>

        <Button
            android:id="@+id/btn_hongli_tiqu_lijitiqu2"
            android:layout_width="match_parent"
            android:layout_height="90px"
            android:layout_below="@+id/linearLayout_ketiqu_hongli"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="30px"
            android:layout_marginRight="30px"
            android:layout_marginTop="100px"
            android:background="@drawable/bg_btn_confirm"
            android:text="立即提取"
            android:textColor="#ffffff"
            android:textSize="36px" />

    </RelativeLayout>
</RelativeLayout>
