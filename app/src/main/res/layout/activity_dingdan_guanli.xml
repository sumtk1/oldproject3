<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/ll_test"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/include8"
        layout="@layout/include_head" />

    <android.support.design.widget.TabLayout
        android:id="@+id/tl_dingdan_gunli_head"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/include8"
        app:tabIndicatorColor="@color/cl_activity_newmain"
        app:tabSelectedTextColor="@color/cl_activity_newmain"
        android:background="@color/cl_white"
        app:tabTextColor="@color/cl_666" />

    <View
        android:id="@+id/v_ddgl_fuxian"
        style="@style/title_view"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_below="@+id/tl_dingdan_gunli_head"
        />
    <com.zyd.wlwsdk.widge.NoScrollViewPager
        android:id="@+id/vp_dingdan_gunli_page"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/v_ddgl_fuxian" />

</RelativeLayout>
